#import "UIButton+CSHLogin.h"
#import "UIColor+CSH.h"

@implementation UIButton (CSHLogin)

- (void)csh_disableWithLoginAppearance {
    self.enabled = NO;
    self.backgroundColor = [UIColor lightGrayColor];
}

- (void)csh_enableWithLoginAppearance {
    self.enabled = YES;
    self.backgroundColor = [UIColor csh_brandColor];
}

@end
